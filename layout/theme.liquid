  var abTarget = document.querySelector('body');
  var abObserver = new MutationObserver(function () {
    if (abTarget.querySelector('.acsb-actions-active-icon')) {
      abObserver.disconnect();
      document.querySelector('.acsb-trigger')
        .click();
    }
  });
  abObserver.observe(abTarget, {
    subtree: true,
    childList: true,
  });

  document.querySelector('.ab-icon__container')
    .addEventListener('click', () => {
      document.querySelector('body')
        .append(abScript);
