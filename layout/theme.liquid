<!-- Mobile Nav Menu Fixes -->
<script defer>
  if (window.innerWidth < 1024) {
    document.querySelectorAll('.header-nav__link')
      .forEach(function (elem) {
        elem.removeAttribute('href');
      });

    document.querySelectorAll('.header-nav__item--main')
      .forEach(function (elem) {
        elem.addEventListener('click', function (e) {

          if (e.target.classList.contains('header-nav__label')) return;
          if (e.target.getAttribute('href')) return;

          const closestDropdown = e.target.closest('.header-nav__item')
            .querySelector('.header-nav__dropdown');

          const closestArrow = e.target.closest('.header-nav__item')
            .querySelector('svg');

          document.querySelectorAll('.header-nav__dropdown')
            .forEach(function (dropdown) {
              dropdown.classList.remove('open-dropdown');
            });

          closestDropdown.classList.add('open-dropdown');

          document.querySelectorAll('.header-nav__dropdown')
            .forEach(function (dropdown) {
              if (!dropdown.classList.contains('open-dropdown')) {
                dropdown.style.display = 'none';
              }
            });

          document.querySelectorAll('.header-nav__item--main svg')
            .forEach(function (dropdownSVG) {
              dropdownSVG.style.transform = 'none';
            });

          if (getComputedStyle(closestDropdown).display !== 'block') {
            closestDropdown.style.display = 'block';
            closestDropdown.classList.add('open-dropdown');
            closestArrow.style.transform = 'rotate(180deg)';
          } else {
            closestDropdown.style.display = 'none';
            closestDropdown.classList.remove('open-dropdown');
            closestArrow.style.transform = 'none';
          }
        });
      });

    document.querySelectorAll('.header-nav__item--main i')
      .forEach(function (elem) {
        elem.addEventListener('click', function(e) {
          e.preventDefault();
          e.stopPropagation();
          e.target.closest('.header-nav__item--main').click();
        });
      });
  }
</script>

