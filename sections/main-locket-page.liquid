                                  <img src="{{ block.settings.image | image_url: 'master' }}" width="166" height="106" loading="lazy"/>
                                {% else %}
                                  <img src="{{ cust_product.featured_image | image_url: 'master' }}" width="166"
                                       height="106" loading="lazy"/>
                                {% endif %}
                              </div>
                              {% if block.settings.product_name != blank %}
                                <h4>{{ block.settings.product_name }}</h4>
                              {% else %}
                                <h4>{{ cust_product.title }}</h4>
                              {% endif %}
                            </div>
                          </li>
                        {% endif %}
                      {% endfor %}
                    {% endfor %}
