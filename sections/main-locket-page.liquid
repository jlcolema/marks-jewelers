                    {%- for block in section.blocks -%}
                      {% assign cust_product = all_products[block.settings.product_s] %}
                      {% for tag in cust_product.tags %}
                        {% if tag contains 'Sub_Lockets' %}
                          <li class="ad_to_cart">
                            <div class="ad_to_cart_id" data-id="{{ cust_product.variants[0].id }}"
