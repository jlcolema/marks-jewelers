                      <div class="panel">
                        <ul class="demo" id="sort_{{ cust_tag }}">
                          {% paginate collections['locket'].products by 1000 %}
                            {% for product in collections['locket'].products %}
                              {% if product.tags contains cust_tag %}
                                <li id="dragable_product"
                                    class="charms_{{ cust_tag }}" {% if product.metafields.custom.priority %} data-id="{{ product.metafields.custom.priority }}"  {% endif %}>
